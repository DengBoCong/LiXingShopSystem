<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.power.dbc.Model.LGoodsEntity" table="l_goods" schema="lixing">
        <id name="id">
            <column name="id" sql-type="int(11)" not-null="true"/>
        </id>
        <property name="brandId">
            <column name="brand_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="title">
            <column name="title" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="titleColor">
            <column name="title_color" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="parameterId">
            <column name="parameter_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="factoryOrigin">
            <column name="factory_origin" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="inventory">
            <column name="inventory" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="inventoryUnit">
            <column name="inventory_unit" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
        <property name="images">
            <column name="images" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
        <property name="originalPrice">
            <column name="original_price" sql-type="decimal(10,2)" precision="10" scale="2" not-null="true"/>
        </property>
        <property name="factoryPrice">
            <column name="factory_price" sql-type="decimal(10,2)" precision="10" scale="2" not-null="true"/>
        </property>
        <property name="guidePrice">
            <column name="guide_price" sql-type="decimal(10,2)" precision="10" scale="2" not-null="true"/>
        </property>
        <property name="buyMinNumber">
            <column name="buy_min_number" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="buyMaxNumber">
            <column name="buy_max_number" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isDeductionInventory">
            <column name="is_deduction_inventory" sql-type="tinyint(2)" not-null="true"/>
        </property>
        <property name="isShelves">
            <column name="is_shelves" sql-type="tinyint(2)" not-null="true"/>
        </property>
        <property name="isHomeRecommended">
            <column name="is_home_recommended" sql-type="tinyint(2)" not-null="true"/>
        </property>
        <property name="content">
            <column name="content" sql-type="mediumtext" not-null="true"/>
        </property>
        <property name="photoCount">
            <column name="photo_count" sql-type="tinyint(3)" not-null="true"/>
        </property>
        <property name="salesCount">
            <column name="sales_count" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="accessCount">
            <column name="access_count" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="video">
            <column name="video" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
        <property name="homeRecommendedImages">
            <column name="home_recommended_images" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
        <property name="addTime">
            <column name="add_time" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="updTime">
            <column name="upd_time" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="afterSalesInstruction">
            <column name="after_sales_instruction" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>