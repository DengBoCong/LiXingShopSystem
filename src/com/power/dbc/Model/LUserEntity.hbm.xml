<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.power.dbc.Model.LUserEntity" table="l_user" schema="lixing">
        <id name="id">
            <column name="id" sql-type="int(11) unsigned" not-null="true"/>
        </id>
        <property name="alipayOpenid">
            <column name="alipay_openid" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="weixinOpenid">
            <column name="weixin_openid" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="baiduOpenid">
            <column name="baidu_openid" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="tinyint(1)" not-null="true"/>
        </property>
        <property name="pwd">
            <column name="pwd" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="username">
            <column name="username" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="mobile">
            <column name="mobile" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="gender">
            <column name="gender" sql-type="tinyint(1)" not-null="true"/>
        </property>
        <property name="avatar">
            <column name="avatar" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
        <property name="province">
            <column name="province" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="city">
            <column name="city" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="birthday">
            <column name="birthday" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(150)" length="150" not-null="true"/>
        </property>
        <property name="integral">
            <column name="integral" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="superior">
            <column name="superior" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="addTime">
            <column name="add_time" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="updTime">
            <column name="upd_time" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="licenceId">
            <column name="licence_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="roleId">
            <column name="role_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="orderCount">
            <column name="order_count" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="sellCount">
            <column name="sell_count" sql-type="decimal(10,2)" precision="10" scale="2" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>